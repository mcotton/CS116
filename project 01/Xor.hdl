/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
    // Put your code here.
    Not(in=a,out=nota);
    Not(in=b,out=notb);
    And(a=a,b=notb,out=wc1);
    And(a=nota,b=b,out=wc2);
    Or(a=wc1,b=wc2,out=out);
}
